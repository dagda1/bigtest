{
  "extends": [
    "@frontside",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 120,
        "singleQuote": true,
        "semi": true,
        "tabWidth": 2,
        "trailingComma": "all"
      }
    ]
  }
}
